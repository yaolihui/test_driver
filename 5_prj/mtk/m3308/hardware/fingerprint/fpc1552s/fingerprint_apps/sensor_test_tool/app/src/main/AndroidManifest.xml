<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fingerprints.sensortesttool"
    android:versionCode="0356"
    android:versionName="3.56">

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="com.fingerprints.extension.SENSOR_TEST" />
    <uses-permission android:name="com.fingerprints.extension.NAVIGATION" />
    <uses-permission android:name="com.fingerprints.extension.SENSE_TOUCH" />

    <application
        android:allowBackup="false"
        android:icon="@drawable/fpc_icon"
        android:label="@string/app_name"
        android:logo="@drawable/fpc_logo">

        <uses-library android:name="com.fingerprints.extension" />

        <activity
            android:name=".activities.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/FingerprintsLightTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".activities.MainActivityFTM"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/FingerprintsLightTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.LogActivity"
            android:screenOrientation="portrait"
            android:theme="@style/FingerprintsLightTheme" />

    </application>
</manifest>
